{
  "name": "gh-notifications",
  "version": "1.1.1",
  "description": "ðŸ“¬ Lightweight (<3KB) Wrapper around the GitHub Notifications API",
  "repository": "intevel/gh-notifications",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "github",
    "notifications",
    "api",
    "wrapper",
    "github-api",
    "github-notifications",
    "github-notifications-api",
    "github-notifications-wrapper",
    "gh-notifications",
    "gh-notifications-api",
    "gh-notifications-wrapper"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "changelogen --OUTPUT=CHANGELOG.md && yarn publish && git add . && git push --follow-tags",
    "test": "vitest run --coverage"
  },
  "devDependencies": {
    "@trantlabs/eslint-config-typescript": "^0.0.2",
    "@vitest/coverage-c8": "^0.29.1",
    "changelogen": "^0.4.1",
    "eslint": "^8.34.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vitest": "^0.29.1"
  },
  "packageManager": "pnpm@7.28.0",
  "dependencies": {
    "ofetch": "^1.0.1"
  }
}
